@model Admin.Exigo.Domain.Paging

@if (Model == null) { return; }


 

<div class="dataTables_paginate paging_full_numbers" id="DataTables_Table_1_paginate">
    @if (Model.CurrentPage != 1)
    {
        <a tabindex="0" class="first paginate_button" href="@Model.QueryString&page=1&savesearch=true">First</a>
        
    }
   @for (int i = Model.FirstIndex; i < Model.LastIndex; i++)
   {
       if (Model.CurrentPage == i)
       {
        <a class="paginate_active">@i</a>
        
       }
       else
       {
        <a href="@Model.QueryString&page=@i&savesearch=true" class="paginate_button">@i</a>
         

       }
   }
    
        
      
</div>