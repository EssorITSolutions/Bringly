@model string
@{Layout = null;}
@if (Model.Contains("location") || Model.Contains("appointment"))
{
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="shadow-box restaurant-info">
            <div class="row">
                @Model
            </div>
        </div>
    </div>
}
else
{
    <div class="col-lg-12 col-md-12 col-sm-12">
        @Model
    </div>
}


